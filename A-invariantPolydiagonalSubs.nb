          RowBox[{"Length", "[", "evals", "]"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
     "Print", "[", "\"\<Eigenvalues on top, eigenvectors below\>\"", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "@", 
      RowBox[{"TableForm", "[", 
       RowBox[{
        RowBox[{"Transpose", "[", "evecs", "]"}], ",", " ", 
        RowBox[{"TableHeadings", " ", "\[Rule]", " ", 
         RowBox[{"{", 
          RowBox[{"None", ",", " ", "evals"}], "}"}]}]}], "]"}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
