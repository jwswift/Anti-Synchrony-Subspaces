      RowBox[{"{", 
       RowBox[{"evals", ",", " ", "evecs"}], "}"}], " ", "=", " ", 
      RowBox[{"Eigensystem", "[", "mat", "]"}]}], ";", 
     RowBox[{"(*", " ", 
      RowBox[{
      "evecs", " ", "is", " ", "a", " ", "list", " ", "of", " ", "evecs", " ",
        "as", " ", "row", " ", "vectors"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"components", " ", "=", " ", 
      RowBox[{"Table", "[", 
